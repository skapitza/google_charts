<T3DataStructure>
    <sheets>
        <Data>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Daten</sheetTitle>
                </TCEforms>
                <type>array</type>
        
                <el>
                    <settings.charttype>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Chart Typ</label>
                            <config>
                                <type>select</type>
                                <maxitems>1</maxitems>
                                <items type='array'>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Graph</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">Pie</numIndex>
                                        <numIndex index="1">2</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">Column</numIndex>
                                        <numIndex index="1">3</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.charttype>
            
                    <settings.sheet>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Google Spreadsheet ID</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.sheet>
                    
                    <settings.data>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Data</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_googlecharts_domain_model_chart</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.data>
                </el>
        
            </ROOT>
        </Data>

        <Options>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Optionen</sheetTitle>
                </TCEforms>
                <type>array</type>
        
                <el>
                    <settings.title>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Title</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.title>
                    
                    <settings.width>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Width</label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                            </config>
                        </TCEforms>
                    </settings.width>
            
                    <settings.height>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Height</label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                            </config>
                        </TCEforms>
                    </settings.height>
                    
                    <settings.haxis>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Beschriftung X-Achse</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.haxis>
                    
                    <settings.vaxis>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Beschriftung Y-Achse</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.vaxis>
                </el>
            </ROOT>
        </Options>
        
        <AdditionalOptions>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Zus√§tzliche Optionen</sheetTitle>
                </TCEforms>
                <type>array</type>
        
                <el>
                    <settings.addoptions>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>Options</label>
                            <config>
                                <type>text</type>
                                <cols>24</cols>
                                <rows>3</rows>
                            </config>
                        </TCEforms>
                    </settings.addoptions>
                </el>
            </ROOT>
        </AdditionalOptions>
    </sheets>
</T3DataStructure>